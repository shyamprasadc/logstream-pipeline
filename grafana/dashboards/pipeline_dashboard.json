{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "panels": [
    {
      "type": "stat",
      "title": "Processed Records",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 4 },
      "targets": [
        {
          "expr": "sum(increase(log_records_processed_total[5m]))",
          "legendFormat": "processed"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Processing Errors",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 4 },
      "targets": [
        {
          "expr": "sum(increase(log_records_errors_total[5m]))",
          "legendFormat": "errors"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Processing Latency (p50/p95)",
      "gridPos": { "x": 0, "y": 4, "w": 24, "h": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum(rate(log_processing_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(log_processing_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ]
    }
  ],
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["pipeline"],
  "templating": { "list": [] },
  "time": { "from": "now-6h", "to": "now" },
  "timezone": "browser",
  "title": "Logstream Pipeline",
  "version": 1
}
